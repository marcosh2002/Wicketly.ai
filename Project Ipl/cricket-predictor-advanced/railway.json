{
  "name": "Wicketly-AI",
  "description": "Cricket Prediction AI Platform",
  "builder": "dockerfile",
  "env": {
    "DATABASE_URL": {
      "description": "PostgreSQL Connection String from Supabase"
    },
    "FLASK_ENV": {
      "description": "Flask environment",
      "value": "production"
    },
    "DEBUG": {
      "description": "Debug mode",
      "value": "False"
    }
  },
  "services": [
    {
      "name": "backend",
      "type": "web",
      "source": "/backend",
      "startCommand": "python app.py"
    }
  ]
}
